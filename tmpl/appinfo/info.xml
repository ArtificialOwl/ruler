<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
	<id>{{app_id}}</id>
	<name>{{app_name}}</name>
	<summary>Summary of {{app_name}}</summary>
	<description><![CDATA[

	Description of {{app_name}}

]]>
	</description>
	<version>0.0.1</version>
	<licence>agpl</licence>
	<author>Maxence Lange</author>
	<namespace>{{app_namespace}}</namespace>
	<documentation>
		<admin>https://github.com/{{github}}/wiki</admin>
	</documentation>
	<category>tools</category>
	<website>https://github.com/{{github}}</website>
	<bugs>https://github.com/{{github}}/issues</bugs>
	<repository>https://github.com/{{github}}.git</repository>
	<!--<screenshot>-->
	<!--</screenshot>-->
	<dependencies>
		<nextcloud min-version="12" max-version="13"/>
	</dependencies>
	<settings>
		<admin>OCA\{{app_namespace}}\Settings\Admin</admin>
		<admin-section>OCA\{{app_namespace}}\Settings\AdminSection</admin-section>
		<personal>OCA\{{app_namespace}}\Settings\Personal</personal>
		<personal-section>OCA\{{app_namespace}}\Settings\PersonalSection</personal-section>
	</settings>
</info>
